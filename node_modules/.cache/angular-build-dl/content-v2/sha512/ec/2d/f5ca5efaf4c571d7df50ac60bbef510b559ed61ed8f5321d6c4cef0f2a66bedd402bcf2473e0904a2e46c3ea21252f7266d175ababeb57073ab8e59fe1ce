class InvitationComponent {
    constructor(eventService, changeDetectorRefs) {
        this.eventService = eventService;
        this.changeDetectorRefs = changeDetectorRefs;
        this.displayedColumns = ['title', 'start', 'end', 'status', 'actions'];
    }
    ngOnInit() {
        this.getUserInvitations();
    }
    getUserInvitations() {
        this.eventService.getUserInvitations().subscribe((res) => {
            this.dataSource = res.data;
        });
    }
    AcceptOrRejectInvite(id, status) {
        this.eventService.updateInvitationStatus(id, status).subscribe((res) => {
            this.dataSource = res.data;
            this.changeDetectorRefs.detectChanges();
        });
